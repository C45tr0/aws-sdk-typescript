{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "outDir": "app/build/test",
    "sourceMap": true
  },
  "exclude": [
    "node_modules",
    "output/typings"
  ],
  "filesGlob": [
    "./test/*.ts"
  ]
}
